{
  "extends": ["@commitlint/config-angular"],
  "rules": {
    "scope-enum": [
      2,
      "always",
      [
        "create-discordx",
        "di",
        "discordx",
        "docs",
        "importer",
        "internal",
        "lava-player",
        "lava-queue",
        "music",
        "pagination",
        "utilities"
      ]
    ],
    "type-enum": [
      2,
      "always",
      [
        "build",
        "chore",
        "ci",
        "docs",
        "feat",
        "fix",
        "refactor",
        "revert",
        "test",
        "types",
        "workflow"
      ]
    ]
  }
}
