{
  "compilerOptions": {
    "emitDecoratorMetadata": false,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "forceConsistentCasingInFileNames": true,
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "noEmit": true,
    "noImplicitAny": true,
    "noUncheckedIndexedAccess": true,
    "rootDir": ".",
    "strict": true,
    "strictNullChecks": true,
    "target": "ESNext"
  }
}
