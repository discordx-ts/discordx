{
  "name": "@discordx/changelog",
  "version": "2.12.0",
  "private": false,
  "description": "changelog generator, written in TypeScript with Node.js",
  "keywords": [
    "discordx",
    "discord",
    "typescript",
    "typescript",
    "javascript",
    "changelog",
    "generator",
    "git",
    "github"
  ],
  "homepage": "https://github.com/discordx-ts/discordx/tree/main/packages/changelog#readme",
  "bugs": {
    "url": "https://github.com/discordx-ts/discordx/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/discordx-ts/discordx.git"
  },
  "license": "Apache-2.0",
  "contributors": [
    "Vijay Meena <vijayymmeena@gmail.com> (https://github.com/samarmeena)"
  ],
  "main": "dist/index.js",
  "type": "module",
  "types": "dist/index.d.ts",
  "bin": "dist/cli.js",
  "files": [
    "dist",
    "CHANGELOG.md",
    "LICENSE.txt",
    "README.md",
    "SECURITY.md",
    "package.json"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format esm --dts",
    "build:typedoc": "npx typedoc src/index.ts --out ../../docs/static/api/changelog"
  },
  "dependencies": {
    "commander": "^12.0.0",
    "lodash": "^4.17.21",
    "prettier": "^3.2.5",
    "tslib": "^2.6.2"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@types/lodash": "^4.17.0",
    "@types/node": "^20.11.30",
    "@types/prettier": "^2.7.3",
    "typescript": "5.4.2"
  }
}
