{
  "name": "@discordx/changelog",
  "version": "0.0.0",
  "private": false,
  "description": "changelog generator, written in TypeScript with Node.js",
  "keywords": [
    "discordx",
    "discord",
    "typescript",
    "typescript",
    "javascript",
    "changelog",
    "generator",
    "git",
    "github"
  ],
  "homepage": "https://github.com/discordx-ts/discordx/tree/main/packages/changelog#readme",
  "bugs": {
    "url": "https://github.com/discordx-ts/discordx/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/discordx-ts/discordx.git"
  },
  "license": "Apache-2.0",
  "contributors": [
    "Vijay Meena <indianoceanroleplay@gmail.com> (https://github.com/oceanroleplay)"
  ],
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "bin": "build/cli.js",
  "files": [
    "build",
    "CHANGELOG.md",
    "LICENSE",
    "package.json"
  ],
  "scripts": {
    "build": "tsc",
    "build:changelog": "npx @discordx/changelog --root=src --tag=changelog-v* --tag-replace=changelog-",
    "build:typedoc": "npx typedoc src/index.ts --out ../../docs/static/api/changelog",
    "type-check": "tsc --noemit"
  },
  "dependencies": {
    "lodash": "^4.17.21",
    "prettier": "^2.7.1",
    "tslib": "^2.4.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.185",
    "@types/node": "^18.7.18",
    "@types/prettier": "^2.7.0",
    "typescript": "4.8.3"
  }
}
