{
  "name": "@discordx/utilities",
  "version": "0.0.0",
  "private": false,
  "description": "Add useful features to discordx",
  "keywords": [
    "discord",
    "typescript",
    "bot",
    "javascript",
    "utilities",
    "discordx",
    "discord.ts"
  ],
  "homepage": "https://github.com/oceanroleplay/discord.ts/tree/main/packages/utilities#readme",
  "bugs": {
    "url": "https://github.com/oceanroleplay/discord.ts/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/oceanroleplay/discord.ts.git"
  },
  "license": "Apache-2.0",
  "contributors": [
    "Harry <indianoceanroleplay@gmail.com> (https://github.com/oceanroleplay)"
  ],
  "exports": {
    ".": {
      "import": "./build/esm/index.js",
      "require": "./build/cjs/index.js"
    }
  },
  "main": "build/cjs/index.js",
  "module": "build/esm/index.js",
  "files": [
    "build",
    "LICENSE",
    "README.md",
    "SECURITY.md",
    "CHANGELOG.md",
    "package.json"
  ],
  "scripts": {
    "build": "npm run build:esm && npm run build:cjs",
    "build:cjs": "tsc --module CommonJs --outDir build/cjs && npm run build:fixup:cjs",
    "build:esm": "tsc && npm run build:fixup:mjs",
    "build:fixup:cjs": "npx renamer --find .js --replace .cjs \"build/cjs/**\"",
    "build:fixup:mjs": "npx renamer --find .js --replace .mjs \"build/esm/**\"",
    "type-check": "tsc --noemit"
  },
  "dependencies": {
    "discord.js": "^13.4.0-dev.1636675518.5e0a7d5",
    "discordx": "^10.0.0-dev.1636568467.16689ed",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.176"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=7.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
