{
  "name": "@discordx/utilities",
  "version": "0.0.0",
  "private": false,
  "description": "Add useful features to discordx",
  "keywords": [
    "discord",
    "typescript",
    "bot",
    "javascript",
    "utilities",
    "discordx",
    "discord.ts"
  ],
  "homepage": "https://github.com/oceanroleplay/discord.ts/tree/main/packages/utilities#readme",
  "bugs": {
    "url": "https://github.com/oceanroleplay/discord.ts/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/oceanroleplay/discord.ts.git"
  },
  "license": "Apache-2.0",
  "contributors": [
    "Harry <indianoceanroleplay@gmail.com> (https://github.com/oceanroleplay)"
  ],
  "exports": {
    ".": {
      "import": "./build/esm/index.mjs",
      "require": "./build/cjs/index.cjs",
      "types": "./build/esm/index.d.mts"
    }
  },
  "main": "build/cjs/index.cjs",
  "module": "build/esm/index.mjs",
  "types": "build/esm/index.d.mts",
  "files": [
    "build",
    "LICENSE",
    "README.md",
    "SECURITY.md",
    "CHANGELOG.md",
    "package.json"
  ],
  "scripts": {
    "build": "tsc && npm run build:cjs",
    "build:cjs": "tsc --module CommonJs --outDir build/cjs && npm run postbuild:cjs",
    "postbuild:cjs": "node ../../scripts/postbuild.mjs --path=build/cjs",
    "type-check": "tsc --noemit"
  },
  "dependencies": {
    "discord.js": "^13.4.0-dev.1637193918.493e4f9",
    "discordx": "^10.0.0-dev.1636568467.16689ed",
    "lodash": "^4.17.21",
    "tslib": "^2.3.1"
  },
  "devDependencies": {
    "@discordx/importer": "^1.1.2",
    "@types/lodash": "^4.14.177",
    "typescript": "^4.5.2"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=7.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
