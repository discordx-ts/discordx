{
  "name": "@discordx/importer",
  "version": "0.0.0",
  "private": false,
  "description": "A solution to import files on the module and commonjs at the same time",
  "keywords": [
    "typescript",
    "javascript",
    "importer",
    "mjs",
    "cjs"
  ],
  "homepage": "https://github.com/oceanroleplay/discord.ts/tree/main/packages/importer#readme",
  "bugs": {
    "url": "https://github.com/oceanroleplay/discord.ts/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/oceanroleplay/discord.ts.git"
  },
  "license": "Apache-2.0",
  "contributors": [
    "Harry <indianoceanroleplay@gmail.com> (https://github.com/oceanroleplay)"
  ],
  "exports": {
    ".": {
      "import": "./build/esm/index.js",
      "require": "./build/cjs/index.js",
      "types": "./build/esm/index.d.ts"
    }
  },
  "main": "build/cjs/index.js",
  "module": "build/esm/index.js",
  "types": "build/esm/index.d.ts",
  "files": [
    "build",
    "CHANGELOG.md",
    "LICENSE",
    "package.json"
  ],
  "scripts": {
    "build": "npm run build:esm && npm run build:cjs",
    "build:cjs": "tsc --p tsconfig.cjs.json && npm run postbuild:cjs",
    "build:esm": "tsc && npm run postbuild:esm",
    "postbuild:cjs": "node ../../scripts/postbuild.mjs --module=cjs --path=build/cjs",
    "postbuild:esm": "node ../../scripts/postbuild.mjs --module=esm --path=build/esm",
    "type-check": "tsc --noemit"
  },
  "dependencies": {
    "glob": "^7.2.0",
    "tslib": "^2.3.1",
    "typescript": "^4.5.2"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
