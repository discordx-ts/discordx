{
  "name": "@discordx/music",
  "version": "0.0.0",
  "private": false,
  "description": "A powerful discord music library written in TypeScript for Node.js",
  "keywords": [
    "discord",
    "discordx",
    "discord.ts",
    "typescript",
    "javascript",
    "bot",
    "music",
    "youtube",
    "spotify"
  ],
  "homepage": "https://github.com/oceanroleplay/discord.ts/tree/main/packages/music#readme",
  "bugs": {
    "url": "https://github.com/oceanroleplay/discord.ts/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/oceanroleplay/discord.ts.git"
  },
  "license": "Apache-2.0",
  "contributors": [
    "Harry <indianoceanroleplay@gmail.com> (https://github.com/oceanroleplay)"
  ],
  "exports": {
    ".": {
      "import": "./build/esm/index.mjs",
      "require": "./build/cjs/index.cjs"
    }
  },
  "main": "./build/cjs/index.cjs",
  "module": "./build/esm/index.mjs",
  "files": [
    "build",
    "LICENSE",
    "README.md",
    "SECURITY.md",
    "CHANGELOG.md",
    "package.json"
  ],
  "scripts": {
    "build": "tsc && npm run build:cjs",
    "build:cjs": "tsc --module CommonJs --outDir build/cjs && npm run postbuild:cjs",
    "postbuild:cjs": "node ../../scripts/postbuild.mjs --path=build/cjs",
    "type-check": "tsc --noemit"
  },
  "dependencies": {
    "@discordjs/opus": "^0.7.0",
    "@discordjs/voice": "^0.7.5",
    "@types/backoff": "^2.5.2",
    "ansi-regex": "^6.0.1",
    "backoff": "^2.5.0",
    "discord.js": "^13.4.0-dev.1637193918.493e4f9",
    "ffmpeg-static": "^4.4.0",
    "libsodium-wrappers": "^0.7.9",
    "lodash": "^4.17.21",
    "prism-media": "^1.3.2",
    "spotify-url-info": "^2.2.3",
    "tslib": "^2.3.1",
    "ytdl-core": "^4.9.1",
    "ytpl": "^2.2.3",
    "ytsr": "^3.5.3"
  },
  "devDependencies": {
    "@discordx/importer": "^1.1.2",
    "@types/lodash": "^4.14.177",
    "discordx": "^10.0.0-dev.1637225035.3602a4c",
    "typescript": "^4.5.2"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=7.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
