{
  "name": "@discordx/music",
  "version": "0.0.0",
  "private": false,
  "description": "A powerful discord music library written in TypeScript for Node.js",
  "keywords": [
    "discord",
    "discordx",
    "discord.ts",
    "typescript",
    "javascript",
    "bot",
    "music",
    "youtube",
    "spotify"
  ],
  "homepage": "https://github.com/oceanroleplay/discord.ts/tree/main/packages/music#readme",
  "bugs": {
    "url": "https://github.com/oceanroleplay/discord.ts/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/oceanroleplay/discord.ts.git"
  },
  "license": "Apache-2.0",
  "contributors": [
    "Harry <indianoceanroleplay@gmail.com> (https://github.com/oceanroleplay)"
  ],
  "exports": {
    ".": {
      "import": "./build/esm/index.js",
      "require": "./build/cjs/index.js",
      "types": "./build/esm/index.d.ts"
    }
  },
  "main": "build/cjs/index.js",
  "module": "build/esm/index.js",
  "types": "build/esm/index.d.ts",
  "files": [
    "build",
    "LICENSE",
    "README.md",
    "SECURITY.md",
    "CHANGELOG.md",
    "package.json"
  ],
  "scripts": {
    "build": "npm run build:esm && npm run build:cjs",
    "build:cjs": "tsc --module CommonJs --outDir build/cjs && npm run postbuild:cjs",
    "build:esm": "tsc && npm run postbuild:esm",
    "postbuild:cjs": "node ../../scripts/postbuild.mjs --module=cjs --path=build/cjs",
    "postbuild:esm": "node ../../scripts/postbuild.mjs --module=esm --path=build/esm",
    "build:fixup": "node ../../scripts/fixup-music.mjs",
    "type-check": "tsc --noemit"
  },
  "dependencies": {
    "@discordjs/opus": "^0.7.0",
    "@discordjs/voice": "^0.7.5",
    "@types/backoff": "^2.5.2",
    "ansi-regex": "^6.0.1",
    "backoff": "^2.5.0",
    "discord.js": "^13.4.0-dev.1637107527.c0ba2d4",
    "ffmpeg-static": "^4.4.0",
    "libsodium-wrappers": "^0.7.9",
    "lodash": "^4.17.21",
    "prism-media": "^1.3.2",
    "spotify-url-info": "^2.2.3",
    "tslib": "^2.3.1",
    "ytdl-core": "^4.9.1",
    "ytpl": "^2.2.3",
    "ytsr": "^3.5.3"
  },
  "devDependencies": {
    "@discordx/importer": "^1.1.2",
    "@types/lodash": "^4.14.177",
    "discordx": "^10.0.0-dev.1636568467.16689ed"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=7.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
