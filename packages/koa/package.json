{
  "name": "@discordx/koa",
  "version": "1.0.0",
  "private": false,
  "description": "Use decorators with Koa and Tsyringe",
  "keywords": [
    "koa",
    "api",
    "tsyringe",
    "framework",
    "decorators",
    "typescript",
    "bot",
    "javascript",
    "utilities",
    "discordx",
    "discord.ts"
  ],
  "homepage": "https://github.com/oceanroleplay/discord.ts/tree/main/packages/koa#readme",
  "bugs": {
    "url": "https://github.com/oceanroleplay/discord.ts/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/oceanroleplay/discord.ts.git"
  },
  "license": "Apache-2.0",
  "contributors": [
    "Harry <indianoceanroleplay@gmail.com> (https://github.com/oceanroleplay)"
  ],
  "exports": {
    ".": {
      "import": "./build/esm/index.js",
      "require": "./build/cjs/index.js"
    }
  },
  "main": "./build/cjs/index.js",
  "module": "./build/esm/index.js",
  "files": [
    "build",
    "LICENSE",
    "README.md",
    "SECURITY.md",
    "CHANGELOG.md",
    "package.json"
  ],
  "scripts": {
    "build": "tsc && npm run build:cjs",
    "build:cjs": "tsc --module CommonJs --outDir build/cjs && npm run postbuild:cjs",
    "postbuild:cjs": "node ../../scripts/postbuild.mjs --path=build",
    "type-check": "tsc --noemit"
  },
  "dependencies": {
    "@discordx/di": "^1.0.1",
    "@discordx/internal": "^1.0.0",
    "tslib": "^2.3.1"
  },
  "devDependencies": {
    "@discordx/importer": "^1.1.2",
    "@koa/router": "^10.1.1",
    "@types/koa": "^2.13.4",
    "@types/koa__router": "^8.0.11",
    "koa": "^2.13.4",
    "typescript": "^4.5.4"
  },
  "peerDependencies": {
    "@koa/router": "*",
    "koa": "*"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=7.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
