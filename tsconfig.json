{
  "compilerOptions": {
    "module": "ESNext",
    "target": "ESNext",
    "noImplicitAny": true,
    "sourceMap": true,
    "strict": true,
    "strictNullChecks": true,
    "noUncheckedIndexedAccess": true,
    "emitDecoratorMetadata": false,
    "experimentalDecorators": true,
    "declaration": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "Node",
    "esModuleInterop": true
  },
  "exclude": []
}
