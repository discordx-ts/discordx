{
  "name": "discordx",
  "version": "0.0.0",
  "description": "Create a discord bot with TypeScript and Decorators!",
  "private": false,
  "main": "build/cjs/index.js",
  "module": "build/esm/index.js",
  "exports": {
    ".": {
      "import": "./build/esm/index.js",
      "require": "./build/cjs/index.js"
    }
  },
  "scripts": {
    "build": "npm run build:esm && npm run build:cjs && npm run build:fixup",
    "build:esm": "tsc",
    "build:cjs": "tsc --outDir build/cjs",
    "build:fixup": "node --loader ts-node/esm scripts/fixup.mjs",
    "type-check": "tsc --noemit",
    "test": "jest --detectOpenHandles",
    "lint": "eslint ./",
    "format": "prettier --write .",
    "format-check": "prettier --check ."
  },
  "bugs": {
    "url": "https://github.com/oceanroleplay/discord.ts/issue",
    "email": "indianoceanroleplay@gmail.com"
  },
  "author": "Owen Calvin | Harry",
  "license": "MIT",
  "homepage": "https://discord-ts.js.org",
  "repository": {
    "type": "git",
    "url": "https://github.com/oceanroleplay/discord.ts"
  },
  "keywords": [
    "typescript",
    "discord",
    "bot",
    "client",
    "api",
    "library",
    "framework",
    "tool",
    "decorators"
  ],
  "dependencies": {
    "discord-api-types": "^0.24.0",
    "discord.js": "^13.4.0-dev.1636502714.b256ae1",
    "glob": "^7.2.0",
    "lodash": "^4.17.21",
    "reflect-metadata": "^0.1.13",
    "tslib": "^2.3.1",
    "tsyringe": "^4.6.0"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/lodash": "^4.14.176",
    "@types/node": "^16.11.7",
    "@types/shelljs": "^0.8.9",
    "@typescript-eslint/eslint-plugin": "^5.3.1",
    "@typescript-eslint/parser": "^5.3.1",
    "eslint": "^8.2.0",
    "eslint-plugin-typescript-sort-keys": "^2.0.0",
    "jest": "^27.3.1",
    "prettier": "2.4.1",
    "shelljs": "^0.8.4",
    "ts-jest": "^27.0.7",
    "ts-node": "^10.4.0",
    "typescript": "^4.4.4"
  }
}
